<title>@ViewBag.Title</title>
<link rel="stylesheet" href="~/Assets/css/amazeui.min.css" />
<link rel="stylesheet" href="~/Assets/css/amazeui.datatables.min.css" />
<link rel="stylesheet" href="~/Assets/css/app.css" />
<link href="~/css/amazeui.chosen.css" rel="stylesheet" />
<link href="~/css/themes/default/default.css" rel="stylesheet" />
<link href="~/js/plugins/code/prettify.css" rel="stylesheet" />
<script src="~/js/jquery-1.7.2.min.js"></script>
<script src="~/Assets/js/amazeui.min.js"></script>
<script src="~/js/kindeditor-all.js"></script>
<script src="~/js/lang/zh-CN.js"></script>
<script src="~/js/plugins/code/prettify.js"></script>
<script src="~/js/common.js"></script>
<script src="~/js/amazeui.chosen.js"></script>
<script src="~/js/dyncFormOptionView.js"></script>
<script>
		var code = "@ViewBag.Code";
		var dataId = "@ViewBag.Id";
        jQuery(function () {
            document.getElementById("dataId").value = dataId;
            document.getElementById("code").value = code;
		});
</script>

<body class="theme-white" style="background-color:#ffffff;">
    <div class="am-u-sm-12 am-u-md-12 am-u-lg-12">
        <div class="widget am-cf">
            <div class="widget-head am-cf">
                <div class="widget-title am-fl">@ViewBag.Title</div>
                <div class="widget-function am-fr">
                    <a href="/DyncForm/FormTable?code=@ViewBag.Code" class="am-icon-reply"></a>
                </div>
            </div>
            <div class="widget-body am-fr">
                @Html.BuliderFormView((IEnumerable<DyncField>)ViewBag.DyncField, (SQLDynamicRow)ViewBag.DynamicData)
            </div>
        </div>
    </div>
    <div id="mapContent" style="display:none;position:absolute" class="am-panel am-panel-default">
        <div class="am-panel-hd">地图选择</div>
        <div id="map" class="am-panel-bd">

        </div>
    </div>

    @*<div class="am-modal am-modal-no-btn" tabindex="-5" id="fileModel">
            <div class="am-modal-dialog" style="background-color:#ffffff">
                <div class="am-modal-hd">
                    <h4 id="modalTitle">文件管理</h4>
                    <a href="javascript: void(0)" class="am-close am-close-spin" data-am-modal-close>&times;</a>
                </div>
                <div class="am-modal-bd">
                    <div id="modalContent">
                        <iframe src="http://localhost:46048/index.aspx?type=file&token=" style="width:100%;height:800px"></iframe>
                    </div>
                </div>
            </div>
        </div>*@
    <script type="text/template" id="addressTemp">
        <style>
            .prov > option, .dist > option {
                color: #000000;
            }

            .city > option {
                color: #000000;
            }
        </style>
        <div id="dropdownCity" class="am-form-group">
            <div class="am-u-sm-4">
                <select name="type" data-placeholder="省" class="prov" id="prov"></select>
            </div>
            <div class="am-u-sm-4">
                <select name="type" data-placeholder="城" class="city" id="city"></select>
            </div>
            <div class="am-u-sm-4">
                <select name="type" data-placeholder="市" class="dist" id="dist"></select>
            </div>
        </div>
    </script>
</body>

